<form action= '/peeps' method='post'>
  <div>
    <label for="peep">Write a peep:</label>
    <input type="text" name="peep" />
  </div>
  <div class="button">
    <button type= "submit">Peep</button>
  </div>
</form>

<head>
<h1> Peeps </h1>
</head>
<body>
<ul id="peeps">
  <% @peeps.each do |peep| %>
    <li>
      Peep: <%= peep.peep %>
      Name: <%= peep.name %>
      Username:
    </li>
  <% end %>
</ul>
</body>
